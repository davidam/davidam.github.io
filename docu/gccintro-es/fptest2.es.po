msgid "#include <stdio.h>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"void \n"
"set_fpu (unsigned int mode)\n"
"{\n"
"  asm (\"fldcw %0\" : : \"m\" (*&mode));\n"
"}\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"int\n"
"main (void)\n"
"{\n"
"  double a = 3.0, b = 7.0, c;\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "#ifdef DOUBLE\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "  set_fpu (0x27F);  /* use double-precision rounding */\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "#endif\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "  c = a / b;\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"  if (c == a / b) {\n"
"    printf (\"comparison succeeds\\n\");\n"
"  } else {\n"
"    printf (\"unexpected result\\n\");\n"
"  }\n"
"  return 0;\n"
"}\n"
msgstr ""
