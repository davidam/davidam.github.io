msgstr "@verbatiminclude wabs.es.c\n"

#. type: example
#, no-wrap
msgid ""
"$ gcc -Wall wabs.c\n"
"$ ./a.out\n"
"x = -3.14 |x| = 3  @r{(incorrect)}\n"
msgstr ""
