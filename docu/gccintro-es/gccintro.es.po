msgstr "-Wcast-qual"

#. type: cindex
#, no-wrap
msgid "qualifiers, warning about overriding by casts"
msgstr "calificadores, aviso de sobrescritura por casts"

#. type: cindex
#, no-wrap
msgid "@code{const}, warning about overriding by casts"
msgstr "@code{const}, aviso de sobreescritura por casts"

#. type: cindex
#, no-wrap
msgid "@option{-Wcast-qual} option, warn about casts removing qualifiers"
msgstr "@option{-Wcast-qual}, opci@'on de aviso de casts eliminando calificadores"

#. type: cindex
#, no-wrap
msgid "@option{Wcast-qual} option, warn about casts removing qualifiers"
msgstr "@option{Wcast-qual}, opci@'on de aviso de casts eliminando calificadores"

#. type: table
