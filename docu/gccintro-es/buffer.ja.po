#. type: Plain text
#, no-wrap
msgid "#ifndef BUFFER_H\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "#define BUFFER_H\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"template <class T> \n"
"class Buffer\n"
"{\n"
"public:\n"
"  Buffer (unsigned int n);\n"
"  void insert (const T & x);\n"
"  T get (unsigned int k) const;\n"
"private:\n"
"  unsigned int i;\n"
"  unsigned int size;\n"
"  T *pT;\n"
"};\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"template <class T> \n"
"Buffer<T>::Buffer (unsigned int n)\n"
"{\n"
"  i = 0;\n"
"  size = n;\n"
"  pT = new T[n];\n"
"};\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"template <class T> \n"
"void \n"
"Buffer<T>::insert (const T & x)\n"
"{\n"
"  i = (i + 1) % size;\n"
"  pT[i] = x;\n"
"};\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"template <class T> \n"
"T \n"
"Buffer<T>::get (unsigned int k) const\n"
"{\n"
"  return pT[(i + (size - k)) % size];\n"
"};\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "#endif /* BUFFER_H */\n"
msgstr ""
